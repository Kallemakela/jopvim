                "rules": [
                    {
                        "description": "Hold x to Delete",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "x"
                                },
                                "parameters": {
                                    "basic.to_delayed_action_delay_milliseconds": 0,
                                    "basic.to_if_held_down_repeat_interval_milliseconds": 20,
                                    "basic.to_if_held_down_threshold_milliseconds": 200
                                },
                                "to": [
                                    {
                                        "key_code": "x"
                                    }
                                ],
                                "to_if_held_down": [
                                    {
                                        "key_code": "delete_or_backspace",
                                        "repeat": true
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Caps Lock to Control when held, Escape when tapped",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "caps_lock"
                                },
                                "to": [
                                    {
                                        "key_code": "left_control"
                                    }
                                ],
                                "to_if_alone": [
                                    {
                                        "key_code": "escape"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Map media keys to cmus-remote",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "f8"
                                },
                                "to": [
                                    {
                                        "shell_command": "/usr/local/bin/cmus-remote --pause"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "f9"
                                },
                                "to": [
                                    {
                                        "shell_command": "/usr/local/bin/cmus-remote --next"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "f7"
                                },
                                "to": [
                                    {
                                        "shell_command": "/usr/local/bin/cmus-remote --prev"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Cmd+shift+ยง saves current position",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "non_us_backslash",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_command",
                                            "right_shift"
                                        ],
                                        "optional": [
                                            "any"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "/usr/local/bin/cliclick p:. > ~/code/sc/cursorfocus/.cliclick_pos.txt"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "cmd + ยง clicks saved position",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "non_us_backslash",
                                    "modifiers": {
                                        "mandatory": [
                                            "right_shift"
                                        ],
                                        "optional": [
                                            "any"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "coords=$(cat ~/code/sc/cursorfocus/.cliclick_pos.txt); /usr/local/bin/cliclick c:$coords"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
